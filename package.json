{
  "name": "aap-sanity",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "start": "NODE_ENV=production node dist/index.cjs",
    "dev": "NODE_ENV=development node dist/index.cjs",
    "build": "esbuild src/index.ts --bundle --outfile=dist/index.cjs --platform=node",
    "prepare": "husky",
    "test": "vitest --run",
    "gentypes": "openapi-typescript http://localhost:8082/openapi.json -o src/types/schema.d.ts"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "prettier": "@navikt/aap-felles-prettier",
  "dependencies": {
    "@navikt/aap-felles-utils": "^2.0.28",
    "@navikt/aap-sanity-schema-types": "^0.0.5",
    "@navikt/oasis": "^3.4.0",
    "@sanity/client": "^6.24.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@navikt/aap-felles-prettier": "^2.0.8",
    "@types/express": "^4.17.21",
    "esbuild": "^0.25.1",
    "eslint": "^9.14.0",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "openapi-typescript": "^7.4.3",
    "prettier": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0",
    "vitest": "^3.0.9"
  },
  "packageManager": "yarn@4.5.1"
}
