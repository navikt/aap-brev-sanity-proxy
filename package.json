{
  "name": "aap-sanity",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "start": "NODE_ENV=production node dist/index.cjs",
    "dev": "nodemon -e ts,tsx --exec \"yarn build && NODE_ENV=development node dist/index.cjs\"",
    "build": "esbuild src/index.ts --bundle --loader:.png=dataurl --outfile=dist/index.cjs --platform=node",
    "prepare": "husky",
    "test": "vitest --run",
    "gentypes": "openapi-typescript http://localhost:8082/openapi.json -o src/types/schema.d.ts"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "prettier": "@navikt/aap-felles-prettier",
  "dependencies": {
    "@navikt/aap-felles-utils": "^5.3.0",
    "@navikt/aap-sanity-schema-types": "^0.1.1",
    "@navikt/oasis": "^3.8.0",
    "@portabletext/react": "^4.0.3",
    "@sanity/client": "^6.24.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@navikt/aap-felles-prettier": "^5.3.0",
    "@types/express": "^4.17.21",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "esbuild": "^0.25.6",
    "eslint": "^9.31.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.10",
    "openapi-typescript": "^7.4.3",
    "prettier": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.41.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "yarn@4.9.2"
}
